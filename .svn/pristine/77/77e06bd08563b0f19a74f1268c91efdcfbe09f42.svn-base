    <li ng-if="authenticationfunction(['ROLE_SUPERADMIN'])"><a href="#/dashboard"> <i class="ti-home"></i><span data-i18n="Dashboard"></span> </a></li>
       <li ng-if="authenticationfunction(['ROLE_SUPERADMIN'])"><a href="" ng-click="reload('user')"> <i class="ti-user"></i><span data-i18n="User Management"></span> </a></li>
       <li ng-if="authenticationfunction(['ROLE_SUPERADMIN','ROLE_SPONSEREDADMIN','ROLE_ADMIN'])"><a href="" ng-click="reload('survey')"> <i class="ti-notepad"></i><span data-i18n="Survey"></span> </a></li>
    </ul> -->
    
    <ul class="nav-left list-unstyled">
           <!--  <li>
                <a href="#/" data-toggle-nav-collapsed-min
                             class="toggle-min"
                             ><i class="ti-menu"></i></a>
            </li> -->
            
            <li  ng-class="{menuactive: isActive('/dashboard')}" ng-if="authenticationfunction(['ROLE_SUPERADMIN'])"><a href="#/dashboard"> <i class="ti-home" style="padding-right: 6px;"></i><span class="visible-xs"></span><span class="  hidden-xs" data-i18n="Dashboard"></span> </a></li>
           
           	<li  ng-class="{menuactive: isActive('/user')}" ng-if="authenticationfunction(['ROLE_SUPERADMIN'])"><a href="" ng-click="reload('user')"> <i class="ti-user" style="padding-right: 6px;"></i><span class="visible-xs"></span><span class="  hidden-xs" data-i18n="User Management"></span> </a></li>
           	
           	<li ng-class="{menuactive: isActive('/survey')}" ng-if="authenticationfunction(['ROLE_SUPERADMIN','ROLE_SPONSEREDADMIN','ROLE_ADMIN'])" ><a href="" ng-click="reload('survey')"><i class="ti-notepad" style="padding-right: 6px;"></i><span class="visible-xs"></span><span class=" hidden-xs" data-i18n="Survey"></span></a></li>
            
        </ul>
    
 
        <ul class="nav-right pull-right list-unstyled">

            <li class="dropdown text-normal nav-profile">
                <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">
                    <img src="data:image/svg+xml;base64," alt="" class="img-circle img30_30" ng-src="{{userdata.resp.imageUrl}}">
                    <span class="hidden-xs hidden-sm">
                        <span data-i18n="{{userdata.resp.name}}"></span>
                    </span>
                </a>
                <ul class="dropdown-menu with-arrow pull-right">
                    <li>
                        <a href="#userprofile/{{userdata.resp.id}}">
                            <i class="ti-user"></i>
                            <span data-i18n="My Profile"></span>
                        </a>
                    </li>
                    <li>
                        <a href="" ng-click="logout()">
                            <i class="ti-export"></i>
                            <span data-i18n="Log Out"></span>
                        </a>
                    </li>
                </ul>
            </li>
   
        </ul>
